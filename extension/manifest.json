{
  "name": "WebQuest",
  "version": "1.0.2",
  "description": "WebQuest Chrome Extension",
  "background_page": "background.html",
  "browser_action" :
  {
    "default_icon" : "icons/default.png",
    "default_popup": "popup.html"
  },
  "permissions" : [
    "tabs",
    "http://localhost:8080/*",
    "http://oni.dyndns.org:8080/*",
    "notifications"
  ],
  "options_page": "options.html",
  "default_locale": "en",
  "icons": {
      "16": "icons/web-quest-16.png",
      "48": "icons/web-quest-48.png",
      "128": "icons/web-quest-128.png"
  }
}